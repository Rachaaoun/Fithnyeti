{% extends 'baseadmin.html.twig' %}

{% block title %}Complaint{% endblock %}

{% block body %}
    <h1>Complaint</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ complaint.id }}</td>
            </tr>
            <tr>
                <th>Object</th>
                <td>{{ complaint.object }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ complaint.description }}</td>
